      "../../../src/*": ["src/*"],
      "@elastic/eui": ["node_modules/@elastic/eui"],
      "@osd/*": ["node_modules/@osd/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "target"]
}
